<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="overheardsite.myapplication.Prayer4">

        <TextView
            android:id="@+id/textView"
            android:layout_width="388dp"
            android:layout_height="287dp"
            android:text="@string/pryr4"
            android:textSize="20sp"
            android:lineSpacingExtra="4dp"
            android:gravity="center"
            android:textColor="@color/navyblue"
            android:fontFamily="sans-serif-condensed"
            tools:layout_editor_absoluteY="95dp"
            tools:layout_editor_absoluteX="11dp" />

        <Button
            android:id="@+id/returnToMainButton"
            android:layout_width="88dp"
            android:layout_height="48dp"
            android:text="Return"
            android:textColor="@color/navyblue"
            android:background="@color/red"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="44dp"
            android:layout_height="27dp"
            android:text="0"
            android:gravity="center_horizontal"
            android:textColor="@color/navyblue"
            android:textSize="13sp"
            tools:layout_editor_absoluteY="16dp"
            tools:layout_editor_absoluteX="288dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="You are praying with"
            android:textSize="13sp"
            android:textColor="@color/navyblue"
            tools:layout_editor_absoluteY="16dp"
            tools:layout_editor_absoluteX="173dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="people."
            android:textSize="13sp"
            android:textColor="@color/navyblue"
            tools:layout_editor_absoluteY="16dp"
            tools:layout_editor_absoluteX="333dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="393dp"
            android:layout_height="198dp"
            android:text="@string/pryr4quote"
            android:gravity="center"
            android:textSize="20sp"
            android:lineSpacingExtra="4dp"
            android:textColor="@color/navyblue"
            android:fontFamily="sans-serif-condensed"
            tools:layout_editor_absoluteY="390dp"
            tools:layout_editor_absoluteX="9dp" />

</android.support.constraint.ConstraintLayout>
